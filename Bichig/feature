feature calt { 
  script mong; 
  lookup calt1 { 
  lookupflag IgnoreMarks;
    sub ML_UE.init' [ML_YA.medi1] [ML_I.medi] by ML_UE.init1;
    sub @consonant_init [ML_OE.medi ML_UE.medi]' by [ML_OE.medi1 ML_UE.medi1];
    sub @consonant_init ML_UE.medi' [ML_YA.medi1] [ML_I.medi] by ML_UE.medi;
    sub ML_NA.medi1' @vowel_medi_fina by ML_NA.medi1;
    sub ML_NA.medi1' @consonant_medi_fina by ML_NA.medi;
    sub [ML_A.init ML_A.medi ML_E.init ML_E.medi ML_O.init ML_O.medi ML_U.init ML_U.medi ML_UE.init1 ML_UE.medi] ML_YA.medi1' ML_I.medi by ML_YA.medi;
    sub ML_YA.medi1' @consonant_medi_fina by ML_YA.medi;
    sub ML_YA.medi1' @vowel_medi_fina by ML_YA.medi1;
    sub ML_DA.medi1' @vowel_medi_fina by ML_DA.medi1;
    sub ML_DA.medi1' @consonant_medi_fina by ML_DA.medi;
    sub ML_GA.init' @male_vowel_medi_fina by ML_GA.init;
    sub ML_GA.medi1' @male_vowel_medi_fina by ML_GA.medi1;
    sub [ML_A.init ML_A.medi ML_O.init ML_O.medi ML_U.init ML_U.medi] ML_GA.medi1' @consonant_medi_fina by ML_GA.medi;
    sub [ML_E.init ML_E.medi ML_OE.init ML_OE.medi ML_UE.init ML_UE.medi ML_EE.init ML_EE.medi] ML_GA.fina' by ML_GA.fina1;
    sub [ML_E.init ML_E.medi ML_OE.init ML_OE.medi ML_UE.init ML_UE.medi] ML_I.medi ML_GA.fina' by ML_GA.fina1;
    sub [ML_E.init ML_E.medi ML_OE.init ML_OE.medi ML_UE.init ML_UE.medi] @consonant_medi ML_I.medi ML_GA.fina' by ML_GA.fina1;
    sub [ML_I.init ML_I.medi] @consonant_medi ML_I.medi ML_GA.fina' by ML_GA.fina1;
    sub @consonant_init ML_I.medi ML_GA.fina' by ML_GA.fina1;
    sub ML_QA.init' [ML_A.medi ML_A.fina ML_O.medi ML_O.fina ML_U.medi ML_U.fina] by ML_QA.init;
    sub ML_QA.medi' [ML_A.medi ML_A.fina ML_O.medi ML_O.fina ML_U.medi ML_U.fina] by ML_QA.medi;
    sub ML_JA.init' MVS by ML_JA.medi1;
    sub [ML_A.fina ML_E.fina]' MVS by [ML_A.fina1 ML_E.fina1];
    sub [ML_NA.medi ML_QA.medi ML_GA.medi ML_JA.medi ML_YA.medi ML_WA.medi]' MVS [ML_A.fina1 ML_E.fina1] by [ML_NA.medi2 ML_QA.medi3 ML_GA.medi2 ML_JA.medi1 ML_YA.medi2 ML_WA.fina1];
    sub ML_NA.medi1' FVS1 MVS [ML_A.fina1 ML_E.fina1] by ML_NA.fina;
    sub NNBSP ML_A.medi' ML_CHA.medi by ML_A.medi2;
    sub NNBSP ML_I.medi ML_YA.medi1' by ML_YA.medi;
    sub NNBSP ML_YA.medi' [ML_I.medi ML_I.fina] by ML_YA.medi;
    sub NNBSP [ML_DA.medi ML_TA.medi ML_LA.medi ML_NA.medi ML_YA.medi] ML_UE.medi' by ML_UE.medi;
    sub NNBSP [ML_U.medi ML_UE.medi]' [space NNBS] by [ML_U.fina ML_UE.fina];
    sub NNBSP [ML_U.medi ML_UE.medi]' [ML_NA.fina ML_DA.fina] by [ML_U.medi ML_UE.medi];
    sub NNBSP ML_DA.medi' by ML_DA.medi1;
    sub NNBSP ML_I.medi' [space NNBS] by ML_I.fina;
    sub ML_NA.medifvs1' @vowel_medi_fina by ML_NA.medi;
    sub @consonant_init ML_I.medi ML_GA.finafvs1' by ML_GA.fina;
    sub ML_DA.medifvs1' @consonant_medi_fina by ML_DA.medi1;
    sub ML_BA.init' ML_OE.medi' by ML_BA_OE.init;
    sub ML_BA.init' ML_A.medi' by ML_BA_A.initmedi;
    sub ML_BA.medi' ML_A.medi' by ML_BA_A.initmedi;
    sub ML_BA.init' ML_A.fina' by ML_BA_A.isolfina;
    sub ML_BA.medi' ML_A.fina' by ML_BA_A.isolfina;
    sub ML_BA.init' ML_I.medi' by ML_BA_I.initmedi;
    sub ML_BA.medi' ML_I.medi' by ML_BA_I.initmedi;
    sub ML_BA.init' ML_I.fina' by ML_BA_I.isolfina;
    sub ML_BA.medi' ML_I.fina' by ML_BA_I.isolfina;
    sub ML_BA.init' ML_O.medi' by ML_BA_O.initmedi;
    sub ML_BA.medi' ML_O.medi' by ML_BA_O.initmedi;
    sub ML_BA.init' ML_O.fina' by ML_BA_O.isolfina;
